<package>
  <description brief="Simple 2D path planner using SBPL">

     humanoid_planner_2d - provides a simple 2D path planner as wrapper around
     SBPL (ARA*, AD*, R*).

  </description>
  <author>Armin Hornung</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/humanoid_planner_2d</url>


  <export>
    <cpp cflags="-I${prefix}/include" lflags="-L${prefix}/lib -Wl,-rpath,${prefix}/lib -lhumanoid_planner_2d"/>
  </export>  

  <depend package="geometry_msgs"/>
  <depend package="roscpp"/>
  <depend package="gridmap_2d"/>
 
  
  <rosdep name="sbpl" />
  
</package>


